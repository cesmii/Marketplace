        "ENABLECLOUDLIBSEARCH": "false",
        "MARKETPLACE_COLLECTION_NAME": "marketplace",
        "MARKETPLACE_MONGODB_CONNECTIONSTRING": "mongodb://testuser:password@127.17.0.2:27017/test?directConnection=true",
        "MARKETPLACE_MONGODB_DATABASE": "test"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "sslPort": 5001
